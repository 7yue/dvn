   text-align: left
}
.list-paging-header {
  background-color: white;
}

.list-paging-footer {
  background-color: white;
}

.list-row-even { 
}

.list-row-odd { 
  background-color: #eeeeee;
}

/* Style rules for message severity levels */

.infoMessage {
  color: black;
}

.successMessage {
    margin: 0px;
    padding: 0px;
    background: #FBD91F;
    color: #002BB8;
    border-width: 1px;
    border-color: #CE8928;
    border-style: solid;
    text-align: center;
}

.warnMessage {
   color: #8B0404;
}

.errorMessage {
  color: red;
  font-weight: bold;
}

.fatalMessage {
  color: red;
  font-style: italic;
  font-weight: bold;
}

/* VDCNet general style rules
*/

body { 
   font-family: Verdana, Arial, Helvetica, sans-serif; 
   margin: 0px; /* Need to set body margin and padding to get consistency between browsers. */
   padding: 0px;
   text-align: center;  /* Counteract to IE/Win Hack */
   font-size: 100%;
}
/* All content jsps have a panel layot (div) with style: vdcSectionMiddle, vdcSectionMiddleNoBorder (when there are more than 1 section columns), 
*  or vdcSectionMiddleFixed (when the section needs to be of a fixed size - specify the size in the jsp)
*/

.vdcSectionMiddle{
   border: 1px solid #cccccc;
   width: 90%;
   margin-left: auto;
   margin-right: auto;
   margin-top: 50px;
   margin-bottom: 50px; 
   text-align: left; /* Counteract to IE/Win Hack */
   height: 100%;
}
.vdcSectionMiddleNoBorder{
   width: 90%;
   margin-left: auto;
   margin-right: auto;
   margin-top: 50px;
   margin-bottom: 50px; 
   padding: 0px;
   text-align: left; /* Counteract to IE/Win Hack */
   height: 100%;   
}
.vdcSectionMiddleFixed {
    margin-left: auto; /* Right and left margin widths set to "auto" */
    margin-right: auto;
    margin-top: 50px;
    margin-bottom: 50px; 
    text-align: left; /* Counteract to IE/Win Hack */
    border: 1px solid #cccccc;
    padding: 0px;
    height: 100%;
}

.vdcSectionMiddleMessage {
    margin-left: auto; /* Right and left margin widths set to "auto" */
    margin-right: auto;
    margin-top: 0px;
    margin-bottom: 0px; 
    text-align: left; /* Counteract to IE/Win Hack */
    padding: 0px;
    height: 100%;
}
